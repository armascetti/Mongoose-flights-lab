<%- include ('../partials/html-head') %>
<link rel ="stylesheet" href="/stylesheet/destinations/new.css"/>
<%- include ('../partials/nav') %>
<main>
  <h1><%= title %></h1>
  <p> Please make sure your Destination is not in the dropdown already
  <select>
    <% destinations.forEach(function(destination){ %>
    <option><%= destination.place %></option>
    <% }) %>
  </select>
  </p>
  <form id="add-destination-form" action="/destinations" method="POST">
<label for="place-input">Name of Place: </label>
<input type="text" name="place" id="place-input" required>
<button type="submit">Add Destination </button>
  </form>
</main>
<%- include('../partials/footer') %>